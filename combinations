def combinations(n,k):
    result=[]
    def backtrack(start,path):
        if len(path)==k:
            result.append(path[:])
            return
        for i in range(start,n+1):
            path.append(i)
            backtrack(i+1,path)
            path.pop()
    backtrack(1,[])
    return result
n=int(input())
k=int(input())
print(combinations(n,k))
